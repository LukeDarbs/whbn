AddDefaultCharset UTF-8

ErrorDocument 404 /404/

# HTTPS redirect for live environment on tsoHost #
# ---------------------------------------------- #

RewriteEngine on
RewriteCond %{SERVER_PORT} 80
RewriteCond %{HTTP_HOST} ^(www\.)?workhardbenice\.co
RewriteRule ^(.*)$ https://www.workhardbenice.co/$1 [R,L]

